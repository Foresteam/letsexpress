{
  "name": "letsexpress",
  "version": "0.0.1",
  "description": "A Quasar Project",
  "productName": "Let's Express",
  "author": "Foresteam <heler1141@gmail.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue --max-warnings=0 ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "prepare": "husky install",
    "precommit": "yarn lint-staged && yarn test",
    "dev": "npx quasar dev",
    "android:dev": "JAVA_HOME=/usr/lib/jvm/java-11-openjdk npx quasar dev -m capacitor -T android",
    "android:build": "JAVA_HOME=/usr/lib/jvm/java-11-openjdk npx quasar build -d -m capacitor -T android",
		"android:install": "adb install dist/capacitor/android/apk/debug/app-debug.apk",
    "android:ide": "JAVA_HOME=/usr/lib/jvm/java-11-openjdk npx quasar dev -m capacitor -T android --ide"
  },
  "lint-staged": {
    "**/*.{vue,ts,js,tsx}": "eslint --fix --max-warnings=0"
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "husky": "^8.0.3",
    "pinia": "^2.0.11",
    "quasar": "^2.6.0",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@quasar/app-vite": "^1.0.0",
    "@types/node": "^12.20.21",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-vue": "^9.0.0",
    "lint-staged": "^13.1.2",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
